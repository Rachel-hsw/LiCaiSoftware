<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="480dip"
  android:layout_height="800dip"
  android:orientation="vertical"
  >
  	<LinearLayout 
		android:id="@+id/LinearLayout01" 
		android:layout_width="480dip" 
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:background="@drawable/line"
		>
		<Button 
			android:text="@string/back" 
			android:id="@+id/back" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:background="@drawable/myselector_back"
			android:layout_marginLeft="20dip"
			android:textColor="@color/white"
			android:gravity="center"
			android:layout_gravity="center"
			>
		</Button>
		<TextView 
			android:text="@string/fangdai" 
			android:id="@+id/calfd" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_marginLeft="50dip"
			android:textSize="20dip"
			android:layout_gravity="center"
			>
		</TextView>
		<Button 
			android:text="@string/jisuan" 
			android:id="@+id/jscal" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_marginLeft="50dip"
			android:background="@drawable/myselector_cal"
			android:textColor="@color/white"
			android:gravity="center"
			android:layout_gravity="center"
			>
		</Button>
		
	</LinearLayout>
	<LinearLayout 
		android:id="@+id/LinearLayout02" 
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:orientation="vertical"
		android:background="@drawable/cal_bg2"
		>
		<LinearLayout 
			android:id="@+id/LinearLayout03" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			>
			<TextView 
				android:text="@string/lt" 
				android:id="@+id/lt" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="30dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="@string/jijin" 
				android:id="@+id/jijin" 
				android:layout_width="110dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="90dip"
				android:layout_marginTop="25dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		
		<LinearLayout 
			android:id="@+id/LinearLayout04" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			>
			<TextView 
				android:text="@string/jijinmoney" 
				android:id="@+id/jijinmoney" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="5dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="0.0" 
				android:id="@+id/jine" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="44dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
			<Button 
				android:text="万元" 
				android:id="@+id/wanyuan" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="1dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		
		<LinearLayout 
			android:id="@+id/LinearLayout05" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			>
			<TextView 
				android:text="@string/loadyears" 
				android:id="@+id/loadyears" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="5dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="20年" 
				android:id="@+id/nian" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="90dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
			<Button 
				android:text="0月" 
				android:id="@+id/yue" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="5dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		
		<LinearLayout 
			android:id="@+id/LinearLayout06" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			>
			<TextView 
				android:text="@string/jijinrate" 
				android:id="@+id/jijinrate" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="5dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="@string/shuru" 
				android:id="@+id/jijininput" 
				android:layout_width="110dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="44dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		
		
		<LinearLayout 
			android:id="@+id/LinearLayout07" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			>
			<Button 
				android:text="0.0" 
				android:id="@+id/jijinlilv" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="180dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
			<Button 
				android:text="%" 
				android:id="@+id/bfh" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="1dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		
		
		<LinearLayout 
			android:id="@+id/LinearLayout08" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:orientation="horizontal"
			>
			<TextView 
				android:text="@string/pt" 
				android:id="@+id/pt" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="5dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="@string/debx" 
				android:id="@+id/hkmode2" 
				android:layout_width="110dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="90dip"
				android:layout_marginTop="5dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		
	

</LinearLayout>

</LinearLayout>
