<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:layout_width="match_parent"
  android:layout_height="match_parent"
  android:orientation="vertical"
  >
  	<LinearLayout 
		android:id="@+id/LinearLayout01" 
		android:layout_width="match_parent" 
		android:layout_height="wrap_content"
		android:orientation="horizontal"
		android:background="@drawable/line"
		>
		<Button 
			android:text="@string/back" 
			android:id="@+id/back" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:background="@drawable/myselector_back"
			android:layout_marginLeft="10dip"
			android:textColor="@color/white"
			android:gravity="center"
			android:layout_gravity="center"
			>
		</Button>
		<TextView 
			android:text="@string/lingcunzhengqu" 
			android:id="@+id/lczq" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_marginLeft="35dip"
			android:textSize="20dip"
			android:layout_gravity="center"
			>
		</TextView>
		<Button 
			android:text="@string/jisuan" 
			android:id="@+id/jscal" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			android:layout_marginLeft="35dip"
			android:background="@drawable/myselector_cal"
			android:textColor="@color/white"
			android:gravity="center"
			android:layout_gravity="center"
			>
		</Button>
		
	</LinearLayout>
	<LinearLayout 
		android:id="@+id/LinearLayout02" 
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical"
		android:background="@drawable/cal_bg2"
		>
		<LinearLayout 
			android:id="@+id/LinearLayout03" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			>
			<TextView 
				android:text="@string/savetime" 
				android:id="@+id/savetime" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="20dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="@string/oneyear" 
				android:id="@+id/oneyear" 
				android:layout_width="110dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="80dip"
				android:layout_marginTop="20dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		<LinearLayout 
			android:id="@+id/LinearLayout04" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			>
			<TextView 
				android:text="@string/savedate" 
				android:id="@+id/savedate" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="1dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="@string/bdate" 
				android:id="@+id/bdate" 
				android:layout_width="110dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="50dip"
				android:layout_marginTop="1dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		<LinearLayout 
			android:id="@+id/LinearLayout05" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			>
			<TextView 
				android:text="@string/eachsaveamt" 
				android:id="@+id/eachsaveamt" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="1dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="0.0" 
				android:id="@+id/jineid" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="48dip"
				android:layout_marginTop="1dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
			<Button 
				android:text="å…ƒ" 
				android:id="@+id/yuan" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="1dip"
				android:layout_marginTop="1dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		<LinearLayout 
			android:id="@+id/LinearLayout06" 
			android:layout_width="wrap_content" 
			android:layout_height="wrap_content"
			>
			<TextView 
				android:text="@string/yearRate" 
				android:id="@+id/yearRate" 
				android:layout_width="wrap_content" 
				android:layout_height="50dip"
				android:layout_marginLeft="25dip"
				android:layout_marginTop="1dip"
				android:textColor="@color/black"
				android:gravity="center"
				android:layout_gravity="center"
				android:textStyle="bold"
				android:textSize="16dip"
				>
			</TextView>
			<Button 
				android:text="3.1" 
				android:id="@+id/nlilv" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="95dip"
				android:layout_marginTop="1dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
			<Button 
				android:text="%" 
				android:id="@+id/bfh" 
				android:layout_width="55dip" 
				android:layout_height="35dip"
				android:layout_marginLeft="1dip"
				android:layout_marginTop="1dip"
				android:layout_gravity="center"
				android:background="@drawable/text"
				android:gravity="center"
				>
			</Button>
		</LinearLayout>
		
	</LinearLayout>

</LinearLayout>

